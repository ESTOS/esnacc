{
	"name": "microservice_client",
	"version": "1.0.1",
	"engines": {
		"npm": ">=9.0.0",
		"node": ">=18.0.0"
	},
	"description": "estos microservice client example",
	"private": true,
	"main": "src/sampleclient.js",
	"scripts": {
		"build:local": "webpack -p",
		"start": "webpack-dev-server",
		"check": "npx madge --circular --extensions ts ./",
		"lint": "eslint --cache --fix -c .eslintrc.precommit.js src/**/*.{js,ts,tsx}",
		"lintclear": "del /s .eslintcache",
		"relint": "run-s lintclear lint",
		"lintnocache": "eslint --fix -c .eslintrc.precommit.js src/**/*.{js,ts,tsx}",
		"depcheck": "npx depcheck"
	},
	"browserslist": [
		"last 3 version",
		">0.5%",
		"not dead",
		"not IE > 0",
		"not op_mini all",
		"not op_mob > 0"
	],
	"dependencies": {
		"@estos/asn1ts": "^3.1.10",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.4"
	},
	"devDependencies": {
		"@types/node": "^18.11.15",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"dotenv-webpack": "^8.0.1",
		"eslint": "^8.29.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-compat": "^4.0.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsdoc": "^40.0.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-standard": "^5.0.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"npm-run-all": "^4.1.5",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.1"
	}
}
