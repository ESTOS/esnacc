{
	"name": "microservice_client",
	"version": "1.0.1",
	"type": "module",
	"engines": {
		"npm": ">=9.0.0",
		"node": ">=18.0.0"
	},
	"description": "estos microservice client example",
	"private": true,
	"main": "src/sampleclient.js",
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"check": "npx madge --circular --extensions ts ./",
		"lint": "dprint fmt && eslint --cache --fix src/**/*.{js,ts,tsx}",
		"lintclear": "del /s .eslintcache",
		"relint": "run-s lintclear lint",
		"depcheck": "npx depcheck"
	},
	"browserslist": [
		"last 3 version",
		">0.5%",
		"not dead",
		"not IE > 0",
		"not op_mini all",
		"not op_mob > 0"
	],
	"dependencies": {
		"@estos/asn1ts": "^3.1.12",
		"ts-loader": "^9.4.2",
		"typescript": "^5.8.3"
	},
	"devDependencies": {
		"@estos/config-eslint": "^1.0.8",
		"@types/node": "^24.1.0",
		"dprint": "^0.50.1",
		"eslint": "^9.31.0",
		"npm-run-all": "^4.1.5",
		"vite": "^7.0.6"
	}
}
